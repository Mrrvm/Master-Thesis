  \item [{$s_x, s_y$}]\begingroup Pixel scalings\nomeqref {0}
		\nompageref{xv}

 \nomgroup{B}

  \item [{$E$}]\begingroup Essential Matrix\nomeqref {0}\nompageref{xv}
