  \item [{$K$}]\begingroup Intrinsics Matrix\nomeqref {0}
		\nompageref{xv}
  \item [{$P$}]\begingroup Projection Matrix\nomeqref {0}
		\nompageref{xv}
  \item [{$\lambda$}]\begingroup Depth\nomeqref {0}\nompageref{xv}
  \item [{$f$}]\begingroup Focal Length\nomeqref {0}\nompageref{xv}
  \item [{$s$}]\begingroup Skew\nomeqref {0}\nompageref{xv}
